function fib(i:float) : float
{
    let lo : float = 0.0;
    let hi : float = 1.0;
    let current : float = 0.0;
    while (current < i) {
        current = current + 1;
        hi = lo + hi;
        lo = hi - lo
    };
    lo
}

function main() : void
{
    let n : float = input();
    print(fib(n))
}
